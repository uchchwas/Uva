#include<stdio.h>
int main()
{
    float M[12][12];char t;
    char ch;float sum=0;int i,j=0;
    scanf("%c",&ch);
    if(ch=='O'){
    for(i=0;i<12;i++){
        for(j=0;j<12;j++){
                scanf("%f",&M[i][j]);

        }
    }
    int K=1;

    for(i=0;i<12;i++){
    j=K;
        for(;j<12;j++){
                sum+=M[i][j];


        }
        K+=1;
    }

    }
    else if(ch=='S'||ch=='M'){
        for(i=0;i<12;i++){
        for(j=0;j<12;j++){
                scanf("%f",&M[i][j]);

        }
    }
    int K=1;

    for(i=0;i<12;i++){
    j=K;
        for(;j<12;j++){
                sum+=M[i][j];


        }
        K+=1;
    }
   sum=sum/66.0;

    }
    printf("%.1f\n",sum);

}
