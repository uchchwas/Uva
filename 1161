#include<stdio.h>
int main()
{
    int M,N;
    while(scanf("%d %d",&M,&N)!=EOF){
     if(M>=0 && M<=20 && N>=0 && N<=20){
        int i,j;long long int mul=1,mul1=1,sum;
        for(i=M;i>0;i--){
            mul*=i;
        }
        for(j=N;j>0;j--){
            mul1*=j;
        }
        sum=mul+mul1;
        printf("%lld\n",sum);
     }

    }
    return 0;
}
