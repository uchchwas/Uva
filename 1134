#include<stdio.h>
int main()
{
    unsigned int al=0,ga=0,di=0;int i;int k=0;

    while(1){scanf("%d",&i);
            {
            if(i==1){
                al++;
            }
            else if(i==2){
                ga++;
            }
            else if(i==3){
                di++;
            }

            else if(i>4){
                k++;
            }
            else if(i==4){
                break;
            }


    }
    }
    printf("MUITO OBRIGADO\nAlcool: %d\nGasolina: %d\nDiesel: %d\n",al,ga,di);
return 0;
}

